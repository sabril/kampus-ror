<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% current_cart.cart_items.each do |item| %>
      <tr>
        <td><%= item.course.title %></td>
        <td><%= number_to_currency item.course.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

